decl
	integer a,b,c,d;
enddecl
integer main(){
	read(a);
	read(b);
	read(c);
	read(d);
	if(a>b) then 
		if(a>c) then 
			if(a>d) then 
				write(a);
			else	
				write(d); 
			endif;
		else 
			if(c>d) then 
			     write(c);
		     	else 
		     		write(d); 
		     	endif;
		endif;
	else
		 if(b>c) then 
			if(b>d) then 
				write(b);
			else 
				write(d); 
			endif;
		else
			if(c>d) then 
				write(c);
		    	else
		    		write(d);
		    	endif; 
		endif;		
	endif;
	return 0;
}
